{
  "name": "blog",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "python3 scripts/compose.py && ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test && open ./coverage/blog/index.html",
    "compose": "python3 scripts/compose.py",
    "lint": "ng lint --fix",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.json --write '**/*.ts'",
    "e2e": "ng e2e",
    "prettier": "npx prettier --write .",
    "dev:ssr": "ng run blog:serve-ssr",
    "serve:ssr": "node dist/blog/server/main.js",
    "build:ssr": "ng build && ng run blog:server",
    "prerender": "ng run blog:prerender",
    "build:docker": "docker build -t blog:latest .",
    "start:docker": "docker run --name blog -p 80:80 blog:latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/platform-server": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "bootstrap": "^5.3.1",
    "express": "^4.18.2",
    "marked": "^7.0.2",
    "ngx-markdown": "^16.0.0",
    "prism": "^4.1.2",
    "prismjs": "^1.29.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.1",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@nguniversal/builders": "^16.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "@testing-library/angular": "^14.2.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jasmine-dom": "^1.3.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.5",
    "@types/marked": "^5.0.1",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "jasmine-core": "^5.1.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "^3.0.1",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
